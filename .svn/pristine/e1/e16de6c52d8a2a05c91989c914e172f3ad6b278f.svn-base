using OliveFramework.Page;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace OliveFramework.method
{
    /// <summary>
    /// PushMessage 的摘要说明
    /// </summary>
    public class PushMessage : BasePage
    {
        protected override void OnRequest()
        {
            base.OnRequest();

            Model.Datatable.MessageModel message = new Model.Datatable.MessageModel();

            try
            {
                message = getModel<Model.Datatable.MessageModel>("message");


            }catch(Exception ex)
            {
                WriteException(ex);
            }

        }
    }
}