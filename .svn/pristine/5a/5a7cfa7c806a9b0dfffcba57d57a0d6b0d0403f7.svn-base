using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace OliveFramework.Model.Datatable
{
    public class AccountModel
    {
        /// <summary>
        /// Account id
        /// </summary>
        public uint? aid { set; get; }
        /// <summary>
        /// Account name
        /// </summary>
        public string Name { set; get; }
        /// <summary>
        /// Account password
        /// </summary>
        public string Password { set; get; }
        /// <summary>
        /// Account mail
        /// </summary>
        public string Mail { set; get; }
        /// <summary>
        /// Account privilege id 
        /// </summary>
        public uint? pid { set; get; }
        /// <summary>
        /// Account group id
        /// </summary>
        public uint? gid { set; get; }
        /// <summary>
        /// User profile id
        /// </summary>
        public uint? profileid { set; get; }
    }
}