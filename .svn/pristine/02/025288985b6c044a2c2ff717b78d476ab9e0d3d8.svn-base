using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace OliveFramework.Controller
{
    public class Merchant: BaseController
    {
        private static Merchant _instance = null;
        public static Merchant Instance
        {
            set
            {

            }
            get
            {
                if (_instance == null)
                {
                    _instance = new Merchant();
                }
                return _instance;
            }
        }



    }
}