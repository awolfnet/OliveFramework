using System;
using System.Collections.Generic;
using System.Web;

namespace OliveFramework.model
{
    public class ResponseModel<T>
    {
        public bool success;
        public T message;

        public ResponseModel()
        {

        }

        public ResponseModel(T t,bool success)
        {
            this.message = t;
            this.success = success;
        }

    }
}